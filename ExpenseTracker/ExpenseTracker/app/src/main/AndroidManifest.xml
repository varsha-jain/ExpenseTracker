<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.expensetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.HomeScreenActivity">

        </activity>
        <activity android:name=".activities.AddExpenseActivity" />
        <activity android:name=".activities.ViewExpenseActivity" />
        <activity android:name=".activities.ViewItemListActivity" />
        <activity android:name=".activities.MainExpenseActivity" />
        <activity android:name=".activities.LoginScreenActivity" />
        <activity android:name=".activities.LaunchingScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>